{
  "name": "aimapper",
  "description": "Universal build tool for managing coding agent rules - Support Claude Code, Cursor, Windsurf, GitHub Copilot, and more",
  "version": "0.2.9",
  "author": "nacyot <propellerheaven@gmail.com>",
  "bin": {
    "aimap": "./bin/run.js"
  },
  "bugs": "https://github.com/nacyot/aimapper/issues",
  "dependencies": {
    "@inquirer/prompts": "^7.7.1",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@types/node": "^18",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./.aimap.example.yml",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/nacyot/aimapper",
  "keywords": [
    "ai",
    "coding-agent",
    "claude",
    "cursor",
    "windsurf",
    "copilot",
    "amazonq",
    "aider",
    "cline",
    "roocode",
    "rules",
    "instructions",
    "cli",
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "aimap",
    "dirname": "aimap",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "nacyot/aimapper",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest",
    "test": "vitest run",
    "test:watch": "vitest",
    "version": "oclif readme && git add README.md",
    "changeset": "changeset",
    "changeset:version": "changeset version && npm install --package-lock-only",
    "changeset:publish": "npm run build && changeset publish",
    "release": "npm run build && npm run test && changeset publish",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm ls debug chalk ansi-styles ansi-regex supports-color strip-ansi wrap-ansi color-convert color-name 2>/dev/null || true",
    "security:install": "npm ci --ignore-scripts",
    "security:update-lock": "npm install --package-lock-only"
  },
  "types": "dist/index.d.ts",
  "overrides": {
    "debug": "^4.4.1 <4.4.2",
    "chalk": "^4.1.2 <5.6.1 || >=5.6.2",
    "ansi-styles": "^4.3.0 <6.2.2 || >=6.2.3",
    "ansi-regex": "^5.0.1 <6.2.1 || >=6.2.2",
    "supports-color": "^8.1.1 <10.2.1 || >=10.2.2",
    "strip-ansi": "^6.0.1 <7.1.1 || >=7.1.2",
    "wrap-ansi": "^6.2.0 <9.0.1 || >=9.0.2",
    "color-convert": "^2.0.1 <3.1.1 || >=3.1.2",
    "color-name": "^1.1.4 <2.0.1 || >=2.0.2",
    "color-string": "<2.1.1 || >=2.1.2",
    "color": "<5.0.1 || >=5.0.2",
    "simple-swizzle": "<0.2.3 || >=0.2.4",
    "is-arrayish": "^0.2.1 <0.3.3 || >=0.3.4",
    "slice-ansi": "<7.1.1 || >=7.1.2",
    "supports-hyperlinks": "<4.1.1 || >=4.1.2",
    "has-ansi": "<6.0.1 || >=6.0.2",
    "chalk-template": "<1.1.1 || >=1.1.2",
    "backslash": "<0.2.1 || >=0.2.2"
  }
}
